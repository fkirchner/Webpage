                %%BIB%%
                %%title%%.
                %%BIB%%(inc:ee;)
                N = "</a>"
              M = ""
              %%BIB%%
              </li>
              <li class="howpub">%%year%%.</li>
              %%BIB%%(inc:abstract;)
              N = "<li class=\"abstract\">%%content%%</li>"
              %%BIB%%
            </ul>
            </article>
            %%ROF%%
