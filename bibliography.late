          <div>
            %%GRP%%(dec: year)
            <a class="EntryGoto" id="%%name%%"></a>
            <h5 class="myheading">%%name%%</h5>
            %%FOR%%(@entry={"article"};)
            <article>
            <ul>
              <li class="type">Journal article</li>
              <li class="coauthors">%%author%%.<br></li>
              <li class="title">
              %%BIB%%(inc:ee;)
              N = "<a href=\"%%content%%\">"
                M = ""
                %%BIB%%
                %%title%%.
                %%BIB%%(inc:ee;)
                N = "</a>"
              M = ""
              %%BIB%%
              </li>
              <li class="howpub">%%journal%% %%volume%%(%%number%%)%%BIB%%(inc:pages;)
              N = ": %%content%%"
              M = ""
              %%BIB%% (%%year%%).</li>
              %%BIB%%(inc:abstract;)
              N = "<li class=\"abstract\">%%content%%</li>"
              %%BIB%%
            </ul>
            </article>
            %%ROF%%

            %%FOR%%(@entry={"inproceedings"};)
            <article>
            <ul>
              <li class="type">Conference paper</li>
              <li class="coauthors">%%author%%.<br></li>
              <li class="title">
              %%BIB%%(inc:ee;)
              N = "<a href=\"%%content%%\">"
                M = ""
                %%BIB%%
                %%title%%.
                %%BIB%%(inc:ee;)
                N = "</a>"
              M = ""
              %%BIB%%
              </li>
              <li class="howpub">%%booktitle%% %%year%%%%BIB%%(inc:pages;)
              N = ": %%content%%"
              M = ""
              %%BIB%%.</li>
              %%BIB%%(inc:abstract;)
              N = "<li class=\"abstract\">%%content%%</li>"
              %%BIB%%
            </ul>
            </article>
            %%ROF%%

            %%FOR%%(@entry={"techreport"};)
            <article>
            <ul>
              <li class="type">Research report</li>
              <li class="coauthors">%%author%%.<br></li>
              <li class="title">
              %%BIB%%(inc:ee;)
              N = "<a href=\"%%content%%\">"
                M = ""
                %%BIB%%
                %%title%%.
                %%BIB%%(inc:ee;)
                N = "</a>"
              M = ""
              %%BIB%%
              </li>
              <li class="howpub">%%institution%%%%BIB%%(inc:number;)
              N = ", %%content%%"
              M = ""
              %%BIB%% (%%year%%).</li>
              %%BIB%%(inc:abstract;)
              N = "<li class=\"abstract\">%%content%%</li>"
              %%BIB%%
            </ul>
            </article>
            %%ROF%%

            %%FOR%%(@entry={"mastersthesis"};)
            <article>
            <ul>
              <li class="type">Master's thesis</li>
              <li class="coauthors">%%author%%.<br></li>
              <li class="title">
              %%BIB%%(inc:ee;)
              N = "<a href=\"%%content%%\">"
                M = ""
                %%BIB%%
                %%title%%.
                %%BIB%%(inc:ee;)
                N = "</a>"
              M = ""
              %%BIB%%
              </li>
              <li class="howpub">%%school%%, %%year%%.</li>
              %%BIB%%(inc:abstract;)
              N = "<li class=\"abstract\">%%content%%</li>"
              %%BIB%%
            </ul>
            </article>
            %%ROF%%

            %%FOR%%(@entry={"phdthesis"};)
            <article>
            <ul>
              <li class="type">Ph.D thesis</li>
              <li class="coauthors">%%author%%.<br></li>
              <li class="title">
              %%BIB%%(inc:ee;)
              N = "<a href=\"%%content%%\">"
                M = ""
                %%BIB%%
                %%title%%.
                %%BIB%%(inc:ee;)
                N = "</a>"
              M = ""
              %%BIB%%
              </li>
              <li class="howpub">%%school%%, %%year%%.</li>
              %%BIB%%(inc:abstract;)
              N = "<li class=\"abstract\">%%content%%</li>"
              %%BIB%%
            </ul>
            </article>
            %%ROF%%

            %%FOR%%(@entry={"misc"};)
            <article>
            <ul>
              <li class="type">Note</li>
              <li class="coauthors">%%author%%.<br></li>
              <li class="title">
              %%BIB%%(inc:ee;)
              N = "<a href=\"%%content%%\">"
                M = ""
                %%BIB%%
                %%title%%.
                %%BIB%%(inc:ee;)
                N = "</a>"
              M = ""
              %%BIB%%
              </li>
              <li class="howpub">%%note%%, %%year%%.</li>
              %%BIB%%(inc:abstract;)
              N = "<li class=\"abstract\">%%content%%</li>"
              %%BIB%%
            </ul>
            </article>
            %%ROF%%

            %%FOR%%(@entry={"manual"};)
            <article>
            <ul>
              <li class="type">Manual</li>
              <li class="coauthors">%%author%%.<br></li>
              <li class="title">
              %%BIB%%(inc:ee;)
